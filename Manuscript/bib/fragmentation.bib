@inproceedings{avecookel:71,
	title = {Single and multiple fracture, The Properties of Fibre Composites},
	abstract = {Single fracture of a composite occurs if, when one of the components fails, the other cannot sustain the additional load placed upon it. This is usually the case for strong stiff fibres in a weak matrix. This paper discusses in detail the more interesting case for brittle matrices (e.g. cement, plaster, glass) where fibres with larger failure strain thean that of the matrix can be introduced. Such compostes fail by multiple fracture. The paper discusses the energeics of multiple fracture in detail and describes the stress/strain curves and hysteresis effects. Et compares the theoretical results with published data for glass in lpaster, steel, glass and asbestos in cement and carbon fibres in glass. The results are in good agreement with experiment and the theory provides a useful guide to the engineering use of these materials.},
	booktitle = {Proc. Conf. National Physical Laboratories},
	publisher = {{IPC} Science and Technology Press Ltd. London},
	author = {Aveston, J. and Cooper, G. and Kelly, A.},
	year = {1971},
	pages = {15--24}
},

@phdthesis{ban:04,
	title = {Bond Behaviour of a Multi-filament Yarn Embedded in a Cementitious Matrix},
	abstract = {Although the bond of a strand in a cementitious matrix is certainly predominated by the bond properties between filaments and matrix, more detailed information is needed to evaluate the failure mechanisms of such a complex system under a pull-out load and hence to allow an analytical and numerical simulation of this composite. Thus, in this study different innovative test methods are developed and used to identify the failure process of a strand as a result of the pull-out process and ascertain the contact faces between the individual filaments and the matrix. Additionally, based on these findings, numerical procedures are proposed to allow, for the first time, to establish a direct relationship between the load history of a pull-out test, to the failure process of an {AR-glass} strand, by means of a mathematical function; the so-called active filament versus displacement relation {NF(\&\#937;).} Together with the load versus displacement relationship P(\&\#937;) also derived during the pull-out
test, an analytical characterization and simulation of the bond between an {AR-glass} strand and a cement based matrix will be possible.},
	author = {Banholzer, B.},
	year = {2004}
},

