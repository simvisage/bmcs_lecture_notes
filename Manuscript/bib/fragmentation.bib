	journal = {Composites Science and Technology},
	author = {Xia, Z. and Okabe, T. and Curtin, {W.A.}},
	year = {2002},
	keywords = {A. Polymer matrix composites ({PMC's)}, Shear-lag model ({SLM)}},
	pages = {1141--1149}
},

@article{gra:84,
